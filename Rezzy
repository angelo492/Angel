local function cleanLocalCharacter(char)
    if not char then return end
    for _, item in ipairs(char:GetChildren()) do
        -- Destruye accesorios, sombreros, mallas, camisas, pantalones, grÃ¡ficos y caras
        if item:IsA("Accessory") or item:IsA("Hat") or item:IsA("ShirtGraphic") or item:IsA("Shirt") or item:IsA("Pants") or item:IsA("CharacterMesh") or item:IsA("Face") then
            item:Destroy()
        end
    end
end
