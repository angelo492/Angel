-- FunciÃ³n efecto rainbow mejorado
local function applyRainbowEffect(textObj)
    task.spawn(function()
        while textObj and textObj.Parent do
            for i = 1, #rainbowColors do
                local c1 = rainbowColors[i]
                local c2 = rainbowColors[(i % #rainbowColors) + 1]
                
                for t = 0, 1, 0.05 do -- MÃ¡s suave
                    if textObj and textObj.Parent then
                        textObj.TextColor3 = c1:Lerp(c2, t)
                        task.wait(0.03) -- MÃ¡s rÃ¡pido
                    else
                        break
                    end
                end
            end
        end
    end)
end

