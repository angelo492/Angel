-- FunciÃ³n para alternar el dropdown
local function toggleDropdown()
    isDropdownOpen = not isDropdownOpen
    
    if isDropdownOpen then
        drop.Visible = true
        TweenService:Create(drop, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Size = UDim2.new(1, 0, 0, targetDropdownHeight)
        }):Play()
    else
        TweenService:Create(drop, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Size = UDim2.new(1, 0, 0, 0)
        }):Play()
        task.wait(0.3)
        drop.Visible = false
    end
end

