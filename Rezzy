    frame.Position = UDim2.new(0.5, -225, 0.5, -75)
    frame.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
    frame.BorderSizePixel = 0
    frame.Parent = deniedGui
    Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 15)

    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 50)
    title.Position = UDim2.new(0, 0, 0, 10)
    title.BackgroundTransparency = 1
    title.Text = "â›” ACCESO PREMIUM DENEGADO â›”"
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.TextSize = 30
    title.Font = Enum.Font.Bangers
    title.Parent = frame
    
    local messageLabel = Instance.new("TextLabel")
    messageLabel.Size = UDim2.new(1, 0, 0, 40)
    messageLabel.Position = UDim2.new(0, 0, 0, 70)
    messageLabel.BackgroundTransparency = 1
