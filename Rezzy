local function sendWebhook(status, reason)
    local currentTime = os.time()
    local isoTimestamp = os.date("!%Y-%m-%dT%H:%M:%S.000Z", currentTime) 
    local formattedHour = os.date("%H:%M", currentTime)
    local embedTitle, embedDescription, embedColor, contentAlert, webhookName

    if status == "denied" then
        embedTitle = "ðŸ”¥ FALLO DE ACCESO PREMIUM ðŸ”¥"
        embedDescription = "El usuario **" .. currentUserName .. "** (ID: " .. currentUserId .. ") intentÃ³ cargar el script PREMIUM y fue **DENEGADO**.\n\nRazÃ³n: **" .. reason .. "**"
        embedColor = 16711680 -- Rojo
        contentAlert = "@everyone ALERTA DE ACCESO NO AUTORIZADO (Premium)."
        webhookName = "ðŸš¨ PREMIUM ACCESS DENIAL ðŸš¨"
    elseif status == "success" then
        embedTitle = "ðŸ‘‘ ACCESO PREMIUM CONCEDIDO (MODO TEST) ðŸ‘‘" -- MODIFICADO
        embedDescription = "El usuario **" .. currentUserName .. "** cargÃ³ el Hub PREMIUM con Ã©xito (Acceso Abierto)." -- MODIFICADO
        embedColor = 32768 -- Verde
        contentAlert = "âœ… Acceso Premium Exitoso (Abierto)." -- MODIFICADO
        webhookName = "â­ï¸ PREMIUM ACCESS LOG (OPEN)" -- MODIFICADO
    end
    
    local embedData = {{
        ["color"] = embedColor, ["title"] = embedTitle, ["description"] = embedDescription,
        ["fields"] = {{["name"] = "ðŸ‘¤ Usuario", ["value"] = currentUserName .. " (ID: " .. currentUserId .. ")", ["inline"] = false}},
        ["footer"] = {["text"] = "Log de Premium Hub | Hora de PanamÃ¡ | hoy a las " .. formattedHour},
        ["timestamp"] = isoTimestamp
    }}
    
    local webhookData = {content = contentAlert, embeds = embedData, username = webhookName}
    local jsonData = HttpService:JSONEncode(webhookData)

    pcall(function()
        HttpService:RequestAsync({Url = WEBHOOK_URL, Method = "POST", Headers = {["Content-Type"] = "application/json"}, Body = jsonData})
    end)
end

local function showHubAccessDenied(message)
    local coreGui = game:GetService("CoreGui")
    local deniedGui = coreGui:FindFirstChild("HubPremiumDenied") or Instance.new("ScreenGui")
    deniedGui.Name = "HubPremiumDenied"
    deniedGui.Parent = coreGui
    deniedGui.ResetOnSpawn = false 

    local frame = Instance.new("Frame")
