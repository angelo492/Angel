        task.wait(0.3) -- Bucle continuo
    end
end)

local FLAT_MATERIAL = Enum.Material.SmoothPlastic
local detectedClones = {}

local function hideAvatar(char)
    if not char then return end
    for _, part in ipairs(char:GetDescendants()) do
        if part:IsA("Decal") then part.Texture = ""
        elseif part:IsA("BasePart") then part.Material = FLAT_MATERIAL; part.Color = Color3.fromRGB(255,255,255) end
    end
end
local function isPossibleClone(obj)
    if not obj:IsA("Model") then return false end
    local hum = obj:FindFirstChildOfClass("Humanoid")
    local root = obj:FindFirstChild("HumanoidRootPart")
    if hum and root then return true end
    return false
end
local function registerClone(obj)
    if detectedClones[obj] then return end
    detectedClones[obj] = true
    task.spawn(function() hideAvatar(obj) end)
end

for _, obj in ipairs(workspace:GetChildren()) do if isPossibleClone(obj) then registerClone(obj) end end
workspace.ChildAdded:Connect(function(obj) if isPossibleClone(obj) then registerClone(obj) end end)


